    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "m1=1\n",
    "m2=1\n",
    "L1=1\n",
    "L2=1\n",
    "Lam=1\n",
    "lam=0.7\n",
    "def V(r):\n",
    "    return L1**2/(2*m1*r**2)+L2**2/(2*m2*(Lam-lam-r)**2)\n",
    "r=np.linspace(0,0.3,100000)\n",
    "v=V(r)\n",
    "plt.plot(r,v)\n",
    "plt.ylim(0,10000)"
   ]
